@model webort.Region

<h3>Are you sure you want to delete this?</h3>
<div>
    <hr />
    <table class="table">
        <tr>
            <td>
               <label>Наименование региона</label>
            </td>

            <td>
                <h4>
                    @Html.DisplayFor(model => model.Region_name)
                </h4>
            </td>
        </tr
    </table>
   

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Удалить" class="btn btn-default" /> |
            @Html.ActionLink("Вернуться назад", "GetRegion")
        </div>
    }
</div>
